{
  "redirect_rule":[{
      "to_uri":"/verynginx/index.html",
      "matcher":"demo_other_verynginx_uri",
      "enable":true
    }],
  "filter_rule":[{
      "enable":false,
      "action":"accept",
      "matcher":"localhost"
    },{
      "enable":true,
      "action":"block",
      "code":"403",
      "matcher":"attack_sql_0"
    },{
      "enable":true,
      "action":"block",
      "code":"403",
      "matcher":"attack_backup_0"
    },{
      "enable":true,
      "action":"block",
      "code":"403",
      "matcher":"attack_scan_0"
    },{
      "enable":true,
      "action":"block",
      "code":"403",
      "matcher":"attack_code_0"
    },{
      "enable":true,
      "code":"200",
      "response":"ip_response",
      "action":"block",
      "custom_response":true,
      "matcher":"ip_matcher"
    },{
      "enable":true,
      "code":"403",
      "response":"white_response",
      "action":"block",
      "custom_response":true,
      "matcher":"white_dvwa"
    },{
      "enable":true,
      "matcher":"wp_cookie",
      "action":"accept"
    },{
      "enable":true,
      "code":"403",
      "response":"wpjson_response",
      "matcher":"wp_none_cookie",
      "custom_response":true,
      "action":"block"
    },{
      "enable":true,
      "code":"403",
      "response":"white_response",
      "matcher":"white_manager",
      "custom_response":true,
      "action":"block"
    },{
      "enable":true,
      "code":"404",
      "response":"sqlinjection_response",
      "matcher":"sql_injection",
      "custom_response":true,
      "action":"block"
    }],
  "config_version":"0.36",
  "static_file_rule":[],
  "proxy_pass_rule":[{
      "enable":true,
      "matcher":"wordpress",
      "upstream":"wp_ssl_up",
      "proxy_host":""
    },{
      "enable":true,
      "matcher":"wordpress",
      "upstream":"wp_http_up",
      "proxy_host":""
    },{
      "enable":true,
      "matcher":"dvwa",
      "upstream":"dvwa_up",
      "proxy_host":""
    }],
  "summary_collect_rule":[],
  "summary_group_persistent_enable":true,
  "admin":[{
      "user":"verynginx",
      "password":"verynginx",
      "enable":true
    }],
  "dashboard_host":"",
  "response":{
    "white_response":{
      "content_type":"text/html",
      "body":"Error : You are not allowed to visit"
    },
    "demo_response_html":{
      "content_type":"text/html",
      "body":"This is a html demo response"
    },
    "wpjson_response":{
      "content_type":"text/html",
      "body":"Error : You are not alllowed to visit this directory"
    },
    "sqlinjection_response":{
      "content_type":"text/html",
      "body":"Error : It is not the valid submission"
    },
    "ip_response":{
      "content_type":"text/html",
      "body":"Error : Can not visit by IP"
    },
    "frelimit_response":{
      "content_type":"text/html",
      "body":"Error : You are not allowed to visit too frenquently"
    },
    "demo_response_json":{
      "content_type":"application/json",
      "body":"{\"msg\":\"soms text\",\"status\":\"success\"}"
    }
  },
  "scheme_lock_enable":false,
  "uri_rewrite_rule":[{
      "to_uri":"/verynginx/$1",
      "matcher":"demo_verynginx_short_uri",
      "replace_re":"^/vn/(.*)",
      "enable":true
    }],
  "cookie_prefix":"verynginx",
  "filter_enable":true,
  "browser_verify_enable":true,
  "scheme_lock_rule":[{
      "enable":false,
      "matcher":"verynginx",
      "scheme":"https"
    }],
  "redirect_enable":true,
  "backend_upstream":{
    "wp_ssl_up":{
      "node":{
        "ssl_Node":{
          "host":"127.0.0.1",
          "weight":"1",
          "scheme":"https",
          "port":"4433"
        }
      },
      "method":"random"
    },
    "wp_http_up":{
      "node":{
        "reverseNode":{
          "host":"127.0.0.1",
          "weight":"1",
          "scheme":"http",
          "port":"8080"
        }
      },
      "method":"random"
    },
    "dvwa_up":{
      "node":{
        "dvwaNode":{
          "host":"127.0.0.1",
          "weight":"1",
          "scheme":"http",
          "port":"8181"
        }
      },
      "method":"random"
    }
  },
  "frequency_limit_enable":true,
  "matcher":{
    "demo_verynginx_short_uri":{
      "URI":{
        "operator":"≈",
        "value":"^/vn"
      }
    },
    "sql_injection":{
      "Host":{
        "value":"dvwa.sec.cuc.edu.cn",
        "operator":"≈"
      },
      "Args":{
        "name_operator":"≈",
        "name_value":"id",
        "operator":"≈",
        "value":"(or|order by|and|union|select.*from)"
      }
    },
    "localhost":{
      "IP":{
        "operator":"=",
        "value":"127.0.0.1"
      }
    },
    "white_dvwa":{
      "IP":{
        "value":"192.168.56.1",
        "operator":"!="
      },
      "Host":{
        "value":"dvwa.sec.cuc.edu.cn",
        "operator":"="
      }
    },
    "wordpress":{
      "Host":{
        "value":"wp.sec.cuc.edu.cn",
        "operator":"="
      }
    },
    "wp_none_cookie":{
      "URI":{
        "value":"/wp-json/wp/v2/users/",
        "operator":"≈"
      }
    },
    "attack_scan_0":{
      "UserAgent":{
        "operator":"≈",
        "value":"(nmap|w3af|netsparker|nikto|fimap|wget)"
      }
    },
    "all_request":{
    },
    "attack_backup_0":{
      "URI":{
        "operator":"≈",
        "value":"\\.(htaccess|bash_history|ssh|sql)$"
      }
    },
    "attack_sql_0":{
      "Args":{
        "name_operator":"*",
        "operator":"≈",
        "value":"select.*from"
      }
    },
    "ip_matcher":{
      "Host":{
        "value":"192.168.56.101",
        "operator":"="
      }
    },
    "verynginx":{
      "URI":{
        "operator":"≈",
        "value":"^/verynginx/"
      }
    },
    "wp_cookie":{
      "URI":{
        "value":"/wp-json/wp/v2/users/",
        "operator":"≈"
      },
      "Cookie":{
        "name_operator":"≈",
        "name_value":"wordpress_logged_in",
        "operator":"Exist"
      }
    },
    "attack_code_0":{
      "URI":{
        "operator":"≈",
        "value":"\\.(git|svn|\\.)"
      }
    },
    "dvwa":{
      "Host":{
        "value":"dvwa.sec.cuc.edu.cn",
        "operator":"="
      }
    },
    "demo_other_verynginx_uri":{
      "URI":{
        "operator":"=",
        "value":"/redirect_to_verynginx"
      }
    },
    "white_manager":{
      "IP":{
        "value":"192.168.56.1",
        "operator":"!="
      },
      "URI":{
        "value":"/verynginx/index.html",
        "operator":"≈"
      }
    }
  },
  "static_file_enable":true,
  "summary_temporary_period":60,
  "summary_group_temporary_enable":true,
  "summary_with_host":false,
  "frequency_limit_rule":[{
      "enable":true,
      "code":"200",
      "response":"frelimit_response",
      "matcher":"dvwa",
      "count":"50",
      "time":"1",
      "custom_response":true,
      "separate":["ip"]
    },{
      "enable":true,
      "code":"200",
      "response":"frelimit_response",
      "matcher":"wordpress",
      "count":"20",
      "time":"1",
      "custom_response":true,
      "separate":["ip"]
    }],
  "uri_rewrite_enable":true,
  "base_uri":"/verynginx",
  "summary_request_enable":true,
  "proxy_pass_enable":true,
  "readonly":false,
  "browser_verify_rule":[{
      "enable":true,
      "matcher":"all_request",
      "type":["cookie","javascript"]
    }]
}
