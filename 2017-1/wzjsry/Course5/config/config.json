{
  "redirect_rule":[{
      "to_uri":"/verynginx/index.html",
      "matcher":"demo_other_verynginx_uri",
      "enable":true
    }],
  "filter_rule":[{
      "enable":false,
      "action":"accept",
      "matcher":"localhost"
    },{
      "enable":true,
      "action":"block",
      "code":"403",
      "matcher":"attack_sql_0"
    },{
      "enable":true,
      "action":"block",
      "code":"403",
      "matcher":"attack_backup_0"
    },{
      "enable":true,
      "action":"block",
      "code":"403",
      "matcher":"attack_scan_0"
    },{
      "enable":true,
      "action":"block",
      "code":"403",
      "matcher":"attack_code_0"
    },{
      "enable":true,
      "code":"404",
      "response":"ip_deny",
      "matcher":"deny_ip",
      "custom_response":true,
      "action":"block"
    },{
      "enable":true,
      "code":"200",
      "response":"dvwa_ip_deny",
      "matcher":"dvwa_ip_deny",
      "custom_response":true,
      "action":"block"
    },{
      "enable":true,
      "code":"404",
      "response":"vn_ip_deny",
      "matcher":"vn_ip_deny",
      "custom_response":true,
      "action":"block"
    },{
      "enable":true,
      "code":"404",
      "response":"demo_response_html",
      "action":"block",
      "custom_response":true,
      "matcher":"buding"
    },{
      "enable":true,
      "code":"200",
      "response":"req_sql",
      "action":"block",
      "custom_response":true,
      "matcher":"sql_in"
    },{
      "enable":true,
      "code":"404",
      "response":"demo_response_html",
      "action":"block",
      "custom_response":true,
      "matcher":"curl_ban"
    }],
  "config_version":"0.36",
  "static_file_rule":[],
  "proxy_pass_rule":[{
      "enable":true,
      "matcher":"all_request",
      "upstream":"dvwa",
      "proxy_host":"dvwa.sec.cuc.edu.cn"
    },{
      "enable":true,
      "matcher":"all_request",
      "upstream":"wordpress",
      "proxy_host":"wp.sec.cuc.edu.cn"
    },{
      "enable":true,
      "matcher":"all_request",
      "upstream":"dvwa_ssl",
      "proxy_host":"https://dvwa.sec.cuc.edu.cn"
    },{
      "enable":true,
      "matcher":"all_request",
      "upstream":"wordpress_ssl",
      "proxy_host":"https://wp.sec.cuc.edu.cn"
    }],
  "summary_collect_rule":[],
  "summary_group_persistent_enable":true,
  "admin":[{
      "user":"verynginx",
      "password":"verynginx",
      "enable":true
    }],
  "dashboard_host":"",
  "response":{
    "dvwa_ip_deny":{
      "content_type":"text/html",
      "body":"you can't browser this site"
    },
    "demo_response_json":{
      "content_type":"application/json",
      "body":"{\"msg\":\"soms text\",\"status\":\"success\"}"
    },
    "req_sql":{
      "content_type":"text/html",
      "body":"sql_inject false"
    },
    "fre_rep":{
      "content_type":"text/html",
      "body":"you visit too fast"
    },
    "demo_response_html":{
      "content_type":"text/html",
      "body":"This is a html demo response"
    },
    "vn_ip_deny":{
      "content_type":"text/html",
      "body":"you can't browser this site"
    },
    "ip_deny":{
      "content_type":"text/html",
      "body":"don't use ip"
    }
  },
  "scheme_lock_enable":false,
  "uri_rewrite_rule":[{
      "to_uri":"/verynginx/$1",
      "matcher":"demo_verynginx_short_uri",
      "replace_re":"^/vn/(.*)",
      "enable":true
    }],
  "cookie_prefix":"verynginx",
  "filter_enable":true,
  "browser_verify_enable":true,
  "scheme_lock_rule":[{
      "enable":false,
      "matcher":"verynginx",
      "scheme":"https"
    }],
  "redirect_enable":true,
  "backend_upstream":{
    "wordpress_ssl":{
      "node":{
        "wordpress_sslN":{
          "host":"192.168.42.3",
          "weight":"11",
          "scheme":"https",
          "port":"2333"
        }
      },
      "method":"random"
    },
    "dvwa":{
      "node":{
        "dvwa_ssl":{
          "host":"192.168.42.3",
          "weight":"11",
          "scheme":"http",
          "port":"333"
        }
      },
      "method":"random"
    },
    "dvwa_ssl":{
      "node":{
        "dvwa_sslN":{
          "host":"192.168.42.3",
          "weight":"11",
          "scheme":"https",
          "port":"3333"
        }
      },
      "method":"random"
    },
    "wordpress":{
      "node":{
        "wordpressN":{
          "host":"192.168.42.3",
          "weight":"11",
          "scheme":"http",
          "port":"233"
        }
      },
      "method":"random"
    }
  },
  "frequency_limit_enable":true,
  "matcher":{
    "curl_ban":{
      "UserAgent":{
        "value":"curl",
        "operator":"="
      }
    },
    "demo_verynginx_short_uri":{
      "URI":{
        "operator":"≈",
        "value":"^/vn"
      }
    },
    "attack_sql_0":{
      "Args":{
        "name_operator":"*",
        "operator":"≈",
        "value":"select.*from"
      }
    },
    "fre_wp":{
      "Host":{
        "value":"wp.sec.cuc.edu.cn",
        "operator":"="
      }
    },
    "demo_other_verynginx_uri":{
      "URI":{
        "operator":"=",
        "value":"/redirect_to_verynginx"
      }
    },
    "sql_in":{
      "Host":{
        "value":"dvwa.sec.cuc.edu.cn",
        "operator":"="
      },
      "Args":{
        "name_operator":"=",
        "name_value":"id",
        "operator":"≈",
        "value":"an|all|select * from|union"
      }
    },
    "dvwa_ip_deny":{
      "IP":{
        "value":"192.168.42.1",
        "operator":"!="
      }
    },
    "attack_scan_0":{
      "UserAgent":{
        "operator":"≈",
        "value":"(nmap|w3af|netsparker|nikto|fimap|wget)"
      }
    },
    "all_request":{
    },
    "vn_ip_deny":{
      "IP":{
        "value":"192.168.42.1",
        "operator":"!="
      }
    },
    "attack_backup_0":{
      "URI":{
        "operator":"≈",
        "value":"\\.(htaccess|bash_history|ssh|sql)$"
      }
    },
    "verynginx":{
      "URI":{
        "operator":"≈",
        "value":"^/verynginx/"
      }
    },
    "localhost":{
      "IP":{
        "operator":"=",
        "value":"127.0.0.1"
      }
    },
    "attack_code_0":{
      "URI":{
        "operator":"≈",
        "value":"\\.(git|svn|\\.)"
      }
    },
    "buding":{
      "Host":{
        "value":"wp.sec.cuc.edu.cn/wp-json/wp/v2/users",
        "operator":"≈"
      }
    },
    "fre_dvwa":{
      "Host":{
        "value":"dvwa.sec.cuc.edu.cn",
        "operator":"="
      }
    },
    "deny_ip":{
      "Host":{
        "value":"\\d+\\.\\d+\\.\\d+\\.\\d+",
        "operator":"≈"
      }
    }
  },
  "static_file_enable":true,
  "summary_temporary_period":60,
  "summary_group_temporary_enable":true,
  "summary_with_host":false,
  "frequency_limit_rule":[{
      "enable":true,
      "code":"200",
      "response":"fre_rep",
      "matcher":"fre_wp",
      "count":"20",
      "time":"1",
      "custom_response":true,
      "separate":["ip"]
    },{
      "enable":true,
      "code":"200",
      "response":"fre_rep",
      "matcher":"fre_dvwa",
      "count":"50",
      "time":"1",
      "custom_response":true,
      "separate":["ip"]
    }],
  "uri_rewrite_enable":true,
  "base_uri":"/verynginx",
  "summary_request_enable":true,
  "proxy_pass_enable":true,
  "readonly":false,
  "browser_verify_rule":[]
}