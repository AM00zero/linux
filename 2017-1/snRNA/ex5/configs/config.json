{
  "redirect_rule":[{
      "to_uri":"/verynginx/index.html",
      "matcher":"demo_other_verynginx_uri",
      "enable":true
    }],
  "filter_rule":[{
      "enable":false,
      "action":"accept",
      "matcher":"localhost"
    },{
      "enable":true,
      "action":"block",
      "code":"403",
      "matcher":"attack_sql_0"
    },{
      "enable":true,
      "action":"block",
      "code":"403",
      "matcher":"attack_backup_0"
    },{
      "enable":true,
      "action":"block",
      "code":"403",
      "matcher":"attack_scan_0"
    },{
      "enable":true,
      "action":"block",
      "code":"403",
      "matcher":"attack_code_0"
    },{
      "enable":true,
      "code":"200",
      "response":"dvwa_white",
      "matcher":"dvwa_white",
      "custom_response":true,
      "action":"block"
    },{
      "enable":false,
      "code":"200",
      "response":"IP_refuse",
      "matcher":"IP_request",
      "custom_response":true,
      "action":"block"
    },{
      "enable":false,
      "code":"200",
      "response":"demo_response_html",
      "matcher":"wp_repair",
      "custom_response":true,
      "action":"block"
    },{
      "enable":true,
      "code":"200",
      "matcher":"curl",
      "custom_response":false,
      "action":"block"
    },{
      "enable":true,
      "code":"403",
      "matcher":"sql_inject",
      "custom_response":false,
      "action":"block"
    },{
      "enable":true,
      "code":"200",
      "response":"verynginx_response_html",
      "action":"block",
      "custom_response":true,
      "matcher":"verynginx_control"
    },{
      "enable":true,
      "matcher":"wp_repair",
      "action":"accept"
    },{
      "enable":true,
      "code":"403",
      "matcher":"wp_repair0",
      "custom_response":false,
      "action":"block"
    }],
  "config_version":"0.36",
  "static_file_rule":[],
  "proxy_pass_rule":[{
      "enable":true,
      "matcher":"wordpress",
      "upstream":"wp_https",
      "proxy_host":""
    },{
      "enable":true,
      "matcher":"wordpress",
      "upstream":"wp_http",
      "proxy_host":""
    },{
      "enable":true,
      "matcher":"dvwa",
      "upstream":"dvwa_http",
      "proxy_host":""
    }],
  "summary_collect_rule":[],
  "summary_group_persistent_enable":true,
  "admin":[{
      "user":"verynginx",
      "password":"verynginx",
      "enable":true
    }],
  "dashboard_host":"",
  "response":{
    "demo_response_json":{
      "content_type":"application/json",
      "body":"{\"msg\":\"soms text\",\"status\":\"success\"}"
    },
    "speed_limit":{
      "content_type":"text/html",
      "body":"your speed is too fast"
    },
    "demo_response_html":{
      "content_type":"text/html",
      "body":"This is a html demo response"
    },
    "verynginx_response_html":{
      "content_type":"text/html",
      "body":"you do not have access to visit the verynginx"
    },
    "dvwa_white":{
      "content_type":"text/html",
      "body":"your ip address is not on the white table of DVWA"
    },
    "IP_refuse":{
      "content_type":"text/html",
      "body":"you are not allowed to pass by ip "
    }
  },
  "scheme_lock_enable":false,
  "uri_rewrite_rule":[{
      "to_uri":"/verynginx/$1",
      "matcher":"demo_verynginx_short_uri",
      "replace_re":"^/vn/(.*)",
      "enable":true
    }],
  "cookie_prefix":"verynginx",
  "filter_enable":true,
  "browser_verify_enable":true,
  "scheme_lock_rule":[{
      "enable":false,
      "matcher":"verynginx",
      "scheme":"https"
    }],
  "redirect_enable":true,
  "backend_upstream":{
    "dvwa_http":{
      "node":{
        "dvwa_http":{
          "host":"127.0.0.1",
          "weight":"1",
          "scheme":"http",
          "port":"5566"
        }
      },
      "method":"random"
    },
    "wp_https":{
      "node":{
        "wp_https":{
          "host":"127.0.0.1",
          "weight":"1",
          "scheme":"https",
          "port":"4433"
        }
      },
      "method":"random"
    },
    "wp_http":{
      "node":{
        "wp_http":{
          "host":"127.0.0.1",
          "weight":"1",
          "scheme":"http",
          "port":"8080"
        }
      },
      "method":"random"
    }
  },
  "frequency_limit_enable":true,
  "matcher":{
    "demo_verynginx_short_uri":{
      "URI":{
        "operator":"≈",
        "value":"^/vn"
      }
    },
    "dvwa_white":{
      "Host":{
        "value":"dvwa.sec.cuc.edu.cn",
        "operator":"="
      },
      "IP":{
        "value":"192.168.56.1",
        "operator":"!="
      }
    },
    "attack_sql_0":{
      "Args":{
        "name_operator":"*",
        "operator":"≈",
        "value":"select.*from"
      }
    },
    "demo_other_verynginx_uri":{
      "URI":{
        "operator":"=",
        "value":"/redirect_to_verynginx"
      }
    },
    "wordpress":{
      "Host":{
        "value":"wp.sec.cuc.edu.cn",
        "operator":"="
      }
    },
    "verynginx_control":{
      "URI":{
        "value":"verynginx",
        "operator":"≈"
      },
      "IP":{
        "value":"192.168.56.1",
        "operator":"!="
      }
    },
    "IP_request":{
      "Host":{
        "value":"\\d+\\.\\d+\\.\\d+\\.\\d+",
        "operator":"≈"
      }
    },
    "attack_scan_0":{
      "UserAgent":{
        "operator":"≈",
        "value":"(nmap|w3af|netsparker|nikto|fimap|wget)"
      }
    },
    "all_request":{
    },
    "attack_backup_0":{
      "URI":{
        "operator":"≈",
        "value":"\\.(htaccess|bash_history|ssh|sql)$"
      }
    },
    "wp_repair0":{
      "URI":{
        "value":"/wp-json/",
        "operator":"≈"
      }
    },
    "dvwa":{
      "Host":{
        "value":"dvwa.sec.cuc.edu.cn",
        "operator":"="
      }
    },
    "sql_inject":{
      "Args":{
        "operator":"≈",
        "name_operator":"*",
        "value":"(union|order|select)"
      }
    },
    "wp_repair":{
      "URI":{
        "value":"/wp-json/",
        "operator":"≈"
      },
      "Cookie":{
        "name_value":"wordpress_logged_in",
        "operator":"Exist",
        "name_operator":"≈"
      }
    },
    "attack_code_0":{
      "URI":{
        "operator":"≈",
        "value":"\\.(git|svn|\\.)"
      }
    },
    "curl":{
      "UserAgent":{
        "value":"curl",
        "operator":"≈"
      }
    },
    "verynginx":{
      "URI":{
        "operator":"≈",
        "value":"^/verynginx/"
      }
    },
    "localhost":{
      "IP":{
        "operator":"=",
        "value":"127.0.0.1"
      }
    }
  },
  "static_file_enable":true,
  "summary_temporary_period":60,
  "summary_group_temporary_enable":true,
  "summary_with_host":false,
  "frequency_limit_rule":[{
      "enable":true,
      "code":"200",
      "response":"speed_limit",
      "time":"1",
      "matcher":"dvwa",
      "count":"50",
      "custom_response":true,
      "separate":["ip"]
    },{
      "enable":true,
      "code":"200",
      "response":"speed_limit",
      "time":"1",
      "matcher":"wordpress",
      "count":"20",
      "custom_response":true,
      "separate":["ip"]
    }],
  "uri_rewrite_enable":true,
  "base_uri":"/verynginx",
  "summary_request_enable":true,
  "proxy_pass_enable":true,
  "readonly":false,
  "browser_verify_rule":[]
}