{
  "redirect_rule":[{
      "to_uri":"/verynginx/index.html",
      "matcher":"demo_other_verynginx_uri",
      "enable":true
    }],
  "filter_rule":[{
      "enable":false,
      "action":"accept",
      "matcher":"localhost"
    },{
      "enable":true,
      "action":"block",
      "code":"403",
      "matcher":"attack_sql_0"
    },{
      "enable":true,
      "action":"block",
      "code":"403",
      "matcher":"attack_backup_0"
    },{
      "enable":true,
      "action":"block",
      "code":"403",
      "matcher":"attack_scan_0"
    },{
      "enable":true,
      "action":"block",
      "code":"403",
      "matcher":"attack_code_0"
    },{
      "enable":false,
      "matcher":"all_request",
      "action":"accept"
    },{
      "enable":true,
      "code":"403",
      "response":"response_ip",
      "matcher":"ip_allowed",
      "custom_response":true,
      "action":"block"
    },{
      "enable":false,
      "code":"403",
      "response":"response_white",
      "matcher":"dvwa_white",
      "custom_response":true,
      "action":"block"
    },{
      "enable":true,
      "code":"406",
      "matcher":"wordpress_volun",
      "custom_response":false,
      "action":"block"
    },{
      "enable":true,
      "code":"403",
      "response":"response_curl",
      "matcher":"curl_banned",
      "custom_response":true,
      "action":"block"
    },{
      "enable":true,
      "code":"406",
      "response":"response_sql",
      "matcher":"dvwa_sql",
      "custom_response":true,
      "action":"block"
    }],
  "config_version":"0.36",
  "static_file_rule":[],
  "proxy_pass_rule":[{
      "enable":true,
      "matcher":"wordpress",
      "upstream":"wordpress",
      "proxy_host":"wp.sec.cuc.edu.cn"
    },{
      "enable":true,
      "matcher":"wordpress",
      "upstream":"wordpress_https",
      "proxy_host":"wp.sec.cuc.edu.cn"
    },{
      "enable":true,
      "matcher":"dvwa",
      "upstream":"dvwa",
      "proxy_host":"dvwa.sec.cuc.edu.cn"
    }],
  "summary_collect_rule":[],
  "summary_group_persistent_enable":true,
  "admin":[{
      "user":"verynginx",
      "password":"verynginx",
      "enable":true
    }],
  "dashboard_host":"",
  "response":{
    "response_ip":{
      "content_type":"text/html",
      "body":"cannot access via IP ! make a try via domain name."
    },
    "response_fre":{
      "content_type":"text/html",
      "body":"your operation is too frequent!"
    },
    "demo_response_json":{
      "content_type":"application/json",
      "body":"{\"msg\":\"soms text\",\"status\":\"success\"}"
    },
    "response_white":{
      "content_type":"text/html",
      "body":"sorry,you don't have the authority ^_^"
    },
    "response_sql":{
      "content_type":"text/html",
      "body":"you can't get away with it!"
    },
    "response_curl":{
      "content_type":"text/html",
      "body":"sorry,curl is unavilable"
    },
    "demo_response_html":{
      "content_type":"text/html",
      "body":"This is a html demo response"
    }
  },
  "scheme_lock_enable":false,
  "uri_rewrite_rule":[{
      "to_uri":"/verynginx/$1",
      "matcher":"demo_verynginx_short_uri",
      "replace_re":"^/vn/(.*)",
      "enable":true
    }],
  "cookie_prefix":"verynginx",
  "filter_enable":true,
  "browser_verify_enable":true,
  "scheme_lock_rule":[{
      "enable":false,
      "matcher":"verynginx",
      "scheme":"https"
    }],
  "redirect_enable":true,
  "backend_upstream":{
    "dvwa":{
      "node":{
        "dvwa":{
          "host":"192.168.63.4",
          "weight":"1",
          "scheme":"http",
          "port":"8080"
        }
      },
      "method":"random"
    },
    "wordpress_https":{
      "node":{
        "wordpress_https":{
          "host":"192.168.63.8",
          "weight":"1",
          "scheme":"https",
          "port":"443"
        }
      },
      "method":"random"
    },
    "wordpress":{
      "node":{
        "wordpress":{
          "host":"192.168.63.8",
          "weight":"1",
          "scheme":"http",
          "port":"80"
        }
      },
      "method":"random"
    }
  },
  "frequency_limit_enable":true,
  "matcher":{
    "curl_banned":{
      "Host":{
        "value":"wp.sec.cuc.edu.cn",
        "operator":"≈"
      },
      "UserAgent":{
        "value":"curl",
        "operator":"≈"
      }
    },
    "demo_verynginx_short_uri":{
      "URI":{
        "operator":"≈",
        "value":"^/vn"
      }
    },
    "dvwa_white":{
      "Host":{
        "value":"dvwa.sec.cuc.edu.cn",
        "operator":"≈"
      }
    },
    "wordpress_volun":{
      "Host":{
        "value":"wp.sec.cuc.edu.cn/wp-json/wp/v2/users",
        "operator":"≈"
      }
    },
    "demo_other_verynginx_uri":{
      "URI":{
        "operator":"=",
        "value":"/redirect_to_verynginx"
      }
    },
    "wordpress":{
      "Host":{
        "value":"wp.sec.cuc.edu.cn",
        "operator":"≈"
      }
    },
    "ip_allowed":{
      "Host":{
        "value":"\\\\d+\\\\.\\\\d+\\\\.\\\\d+\\\\.\\\\d+",
        "operator":"≈"
      }
    },
    "dvwa_sql":{
      "Host":{
        "value":"dvwa.sec.cuc.edu.cn",
        "operator":"≈"
      },
      "Args":{
        "name_operator":"=",
        "name_value":"id",
        "operator":"≈",
        "value":"(and|or|select* from|union)"
      }
    },
    "wp_fre":{
      "Host":{
        "value":"wp.sec.cuc.edu.cn",
        "operator":"≈"
      }
    },
    "dvwa_fre":{
      "Host":{
        "value":"dvwa.sec.cuc.edu.cn",
        "operator":"≈"
      }
    },
    "all_request":{
    },
    "attack_backup_0":{
      "URI":{
        "operator":"≈",
        "value":"\\.(htaccess|bash_history|ssh|sql)$"
      }
    },
    "verynginx":{
      "URI":{
        "operator":"≈",
        "value":"^/verynginx/"
      }
    },
    "localhost":{
      "IP":{
        "operator":"=",
        "value":"127.0.0.1"
      }
    },
    "attack_code_0":{
      "URI":{
        "operator":"≈",
        "value":"\\.(git|svn|\\.)"
      }
    },
    "dvwa":{
      "Host":{
        "value":"dvwa.sec.cuc.edu.cn",
        "operator":"≈"
      }
    },
    "attack_scan_0":{
      "UserAgent":{
        "operator":"≈",
        "value":"(nmap|w3af|netsparker|nikto|fimap|wget)"
      }
    },
    "attack_sql_0":{
      "Args":{
        "name_operator":"*",
        "operator":"≈",
        "value":"select.*from"
      }
    }
  },
  "static_file_enable":true,
  "summary_temporary_period":60,
  "summary_group_temporary_enable":true,
  "summary_with_host":false,
  "frequency_limit_rule":[{
      "enable":true,
      "separate":["ip"],
      "code":"200",
      "response":"response_fre",
      "count":"50",
      "matcher":"dvwa_fre",
      "custom_response":true,
      "time":"1"
    },{
      "enable":true,
      "separate":["ip"],
      "code":"200",
      "response":"response_fre",
      "count":"20",
      "matcher":"wp_fre",
      "custom_response":true,
      "time":"1"
    }],
  "uri_rewrite_enable":true,
  "base_uri":"/verynginx",
  "summary_request_enable":true,
  "proxy_pass_enable":true,
  "readonly":false,
  "browser_verify_rule":[]
}